generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        BigInt   @id
  class     Class?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

enum Class {
  CLASS_11A
  CLASS_11B
}
